    <Controls:MetroWindow.RightWindowCommands>
        <Controls:WindowCommands>
            <Button  Click="Button_Click">
                <StackPanel Orientation="Horizontal">
                    <iconpakges:PackIconMaterial Width="24" Height="24" Kind="Settings" />
                    <TextBlock Margin="4 0 0 0" VerticalAlignment="Center" Text="Setting" />
                </StackPanel>
            </Button>
            <Button MouseDown="Button_MouseDown1">
                <StackPanel Orientation="Horizontal" >
                    <iconpakges:PackIconMaterial Width="24" Height="24" Kind="Account" />
                    <TextBlock Margin="4 0 0 0" VerticalAlignment="Center" Text="Login" />
                </StackPanel>
            </Button>
            <ToggleButton Margin="4 0 0 4"
 Style="{StaticResource MaterialDesignActionToggleButton}"
  ToolTip="MaterialDesignActionLightToggleButton" Command="{Binding SetLanguage}">
                <ToggleButton.Content >
                    <StackPanel>
                        <TextBlock> 中文 </TextBlock>
                        <Button Command="{Binding login}"/>
                    </StackPanel>
                </ToggleButton.Content>
                <materialDesign:ToggleButtonAssist.OnContent>
                    <StackPanel>
                        <TextBlock> US</TextBlock>
                        <Button Command="{Binding login}"/>
                    </StackPanel>
                </materialDesign:ToggleButtonAssist.OnContent>
            </ToggleButton>
        </Controls:WindowCommands>
    </Controls:MetroWindow.RightWindowCommands>
    <Controls:MetroWindow.Flyouts>
        <Controls:FlyoutsControl   >
            <Controls:Flyout  Header="{ local:MyResource Key=Login}"
                             Position="Right" 
                              Theme="Adapt"
                              >
                <Grid Width="400" Margin="10">
                    <Grid.RowDefinitions>

                        <RowDefinition Height="auto"></RowDefinition>
                        <RowDefinition Height="auto"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <StackPanel >
                        <TextBlock >
                        <iconpakges:PackIconMaterial Margin="0 ,12,12,10" Kind="Account" />
                            <Label Content="User_Name" HorizontalAlignment="Center" Margin="0 ,12,12,10"/>
                        </TextBlock>
                        <TextBox SpellCheck.IsEnabled="True" Text="{Binding User_Name}">
                            <TextBox.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Command="ApplicationCommands.New" />
                                    <MenuItem Command="ApplicationCommands.Delete" />
                                    <MenuItem Command="ApplicationCommands.Print" />
                                </ContextMenu>
                            </TextBox.ContextMenu>
                        </TextBox>
                    </StackPanel>
                    <StackPanel Grid.Row="1">
                        
                        <TextBlock >
                        <iconpakges:PackIconMaterial Margin="0 ,12,12,10" Kind="Passport" />
                            <Label Content="Pass" HorizontalAlignment="Center" Margin="0 ,12,12,10"/>
                        </TextBlock>
                        <PasswordBox Margin="{StaticResource ControlMargin}"
                             Controls:TextBoxHelper.ClearTextButton="{Binding RelativeSource={RelativeSource Self}, Path=(Controls:TextBoxHelper.HasText), Mode=OneWay}"
                             Controls:TextBoxHelper.IsWaitingForData="True"
                             Controls:TextBoxHelper.UseFloatingWatermark="True"
                             Controls:TextBoxHelper.Watermark="Revealed PasswordBox"
                             Style="{StaticResource MetroButtonRevealedPasswordBox}"
                                    
                                     cal:Message.Attach="[Event KeyUp]=[Action MyClick($MySender,NULL)]"/>
                    </StackPanel>
                    <StackPanel Grid.Row="2">
                        <TextBlock  >
                        <iconpakges:PackIconMaterial Margin="0 ,12,12,10" Kind="AccountCheck" Height="30"  Width="30"/>
                            <Button Width="100"
                          Margin="{StaticResource ControlMargin}"
                          Content="Check In" 
                           Command="{Binding CheakIn}"
                                    materialDesign:RippleAssist.Feedback="#DD000000"
                                    Style="{StaticResource MaterialDesignFlatButton}" 
                                           />
                             <iconpakges:PackIconMaterial Margin="30 ,12,12,10" Kind="AccountCheck" Height="30"  Width="30"/>
                            <Button Width="100"
                    Margin="{StaticResource ControlMargin}"
                    Content="Check Out"
                   Style="{StaticResource MaterialDesignFlatButton}" />
                        </TextBlock>
                    </StackPanel>

                   
                </Grid>


            </Controls:Flyout>
            <Controls:Flyout Header="{ local:MyResource Key=Window_Seting}"
                             Position="Right"
                             Theme="Adapt">
                <Grid Width="400" Margin="10">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <StackPanel>
                        <ScrollViewer Grid.Column="1"
                      MinWidth="230"
                      Margin="10 10 5 10"
                      Padding="0 0 5 0"
                      HorizontalContentAlignment="Stretch"
                      HorizontalScrollBarVisibility="Disabled">
                            <StackPanel Margin="0 0 0 10" Orientation="Vertical">
                                <Controls:ToggleSwitch Margin="0 10 0 5"
                                       Header="{ local:MyResource Key=IsPaneOpen}"
                                       IsChecked="{Binding ElementName=HamburgerMenuControl, Path=IsPaneOpen, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                <Controls:MetroHeader Margin="0 10 0 5" Header="{ local:MyResource Key=DisplayMode}">
                                    <ComboBox MinWidth="150"
                              HorizontalAlignment="Left"
                              ItemsSource="{Binding Source={StaticResource DisplayModeEnumValues}}"
                              SelectedItem="{Binding ElementName=HamburgerMenuControl, Path=DisplayMode, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                </Controls:MetroHeader>

                                <Controls:MetroHeader Margin="0 10 0 5" Header="{ local:MyResource Key=SelectedIndex}">
                                    <Slider AutoToolTipPlacement="TopLeft"
                            Maximum="3"
                            Minimum="0"
                            Value="{Binding ElementName=HamburgerMenuControl, Path=SelectedIndex, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                </Controls:MetroHeader>

                                <Controls:MetroHeader Margin="0 10 0 5" Header="{ local:MyResource Key=CompactPaneLength}">
                                    <Slider AutoToolTipPlacement="TopLeft"
                            Maximum="80"
                            Minimum="10"
                            Value="{Binding ElementName=HamburgerMenuControl, Path=CompactPaneLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                </Controls:MetroHeader>

                                <Controls:MetroHeader Margin="0 10 0 5" Header="{ local:MyResource Key=OpenPaneLength}">
                                    <Slider AutoToolTipPlacement="TopLeft"
                            Maximum="400"
                            Minimum="50"
                            Value="{Binding ElementName=HamburgerMenuControl, Path=OpenPaneLength, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                </Controls:MetroHeader>

                                <Controls:MetroHeader Margin="0 10 0 5" Header="{ local:MyResource Key=HamburgerVisibility }">
                                    <ComboBox MinWidth="150"
                              HorizontalAlignment="Left"
                              ItemsSource="{Binding Source={StaticResource VisibilityEnumValues}}"
                              SelectedItem="{Binding ElementName=HamburgerMenuControl, Path=HamburgerVisibility, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                </Controls:MetroHeader>

                                <Controls:MetroHeader Margin="0 10 0 5" Header="{ local:MyResource Key=Hamburger Width / Height }">
                                    <UniformGrid Columns="2">
                                        <Slider Margin="0 0 5 0"
                                AutoToolTipPlacement="TopLeft"
                                Maximum="80"
                                Minimum="10"
                                Value="{Binding ElementName=HamburgerMenuControl, Path=HamburgerWidth, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                        <Slider Margin="5 0 0 0"
                                AutoToolTipPlacement="TopLeft"
                                Maximum="80"
                                Minimum="10"
                                Value="{Binding ElementName=HamburgerMenuControl, Path=HamburgerHeight, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                    </UniformGrid>
                                </Controls:MetroHeader>
                            </StackPanel>
                        </ScrollViewer>
                    </StackPanel>
                    <StackPanel Grid.Row="1">
                        <Grid Width="400">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>
                        </Grid>
                        <TextBlock Text="{ local:MyResource Key=Window_Thems}" Grid.Row="0"></TextBlock>
                        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal" Margin="8" Grid.Row="1" Width="400">
                            <TextBlock VerticalAlignment="Center">Light</TextBlock>
                            <ToggleButton Margin="8 0 16 0" Command="{Binding ToggleBaseCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=IsChecked}" />
                            <TextBlock VerticalAlignment="Center">Dark</TextBlock>
                        </StackPanel>
                        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal" Margin="8" Grid.Row="2">
                            <TextBlock VerticalAlignment="Center" Text="{ local:MyResource Key=Window_Thems1}"></TextBlock>
                            <Button Margin="8 0 0 0"  Style="{StaticResource MaterialDesignRaisedAccentButton}"
                                    Command="{Binding ButtonThem0Command}" CommandParameter="{Binding RelativeSource={RelativeSource Self}}" />
                        </StackPanel>
                        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal" Margin="8" Grid.Row="3">
                            <TextBlock VerticalAlignment="Center" Text="{ local:MyResource Key=Window_Thems2}"></TextBlock>
                            <Button Margin="8 0 0 0" Style="{StaticResource MaterialDesignRaisedAccentButton}" 
                                    Command="{Binding ButtonThem1Command}" CommandParameter="{Binding RelativeSource={RelativeSource Self}}"  />
                        </StackPanel>
                    </StackPanel>
                </Grid>
